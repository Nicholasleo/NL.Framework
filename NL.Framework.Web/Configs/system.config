  <add key="SoftName" value="NLFrame管理系统" />
  <!-- 软件版本 -->
  <add key="Version" value="V1.0.0.0" />
  <!-- 软件授权码 -->
  <add key="LicenceKey" value="83eda07f-ead3-4050-bcee-4f41e57a7e16" />
